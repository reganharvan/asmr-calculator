<!DOCclass html>
<html>
	<head>
		<title>ASMR Calculator</title>
		<link rel="icon" href="assets/icon.svg">
		<link rel="stylesheet" href="style.css">
		<audio src="assets/click1.mp3" id="click1"></audio>
		<audio src="assets/click2.mp3" id="click2"></audio>
	</head>
	<body>
		<div class="container">
			<div class="screen-container">
				<div class="symbol-and-text-container">
					<p class="symbol-placeholder" id="symbol-placeholder"></p>
					<p class="text-placeholder" id="text-placeholder">0</p>
				</div>
				<p class="temporary-result-placeholder" id="temporary-result-placeholder"></p>
			</div>
			
			<div class="button-container">
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="7">7
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="8">8
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="9">9
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button operation" class="operation-symbol" id="divide"><p class="operation-symbol-text">&#247 </p>
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="4">4
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="5">5
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="6">6
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button operation" class="operation-symbol" id="multiply"><p class="operation-symbol-text">&times </p>
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="1">1
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="2">2
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="3">3
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button operation" class="operation-symbol" id="minus"><p class="operation-symbol-text">&#45 </p>
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button" id="reset-button">C
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button number" id="0">0
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button result" class="operation-symbol" id="result-button"><p class="operation-symbol-text">&#61 </p>
				</button></div></div>
				
				<div class="button-shadow"><div class="button-stroke">
					<button class="button operation" class="operation-symbol" id="plus"><p class="operation-symbol-text">&#43 </p>
				</button></div></div>
			</div>
		</div>
		<p>Made by Regan Harvan</p>
		<script src="script.js"></script>
	</body>
</html>